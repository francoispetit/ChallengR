<ul>
 <% if @existing_chats_users %>
   <% @existing_chats_users.each do |other_user| %>
    <% if other_user != current_user %>
      <li>
        <%= link_to chat_path(current_user, :other_user => other_user.id) do %>
        <%= other_user.username %>
        <% end %>
      </li>
     <% end %>
   <% end %>
  <% end %>
</ul>
All Users:
<ul>
  <% User.all.each do |other_user| %>
   <% if other_user != current_user %>
     <li>
       <%= link_to chat_path(current_user, :other_user => other_user.id) do %>
       <%= other_user.username %>
       <% end %>
     </li>
    <% end %>
  <% end %>
</ul>
<%= link_to "Logout",  destroy_user_session_path(current_user.id), method: :delete  %>